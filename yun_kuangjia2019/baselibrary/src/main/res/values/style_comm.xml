<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="logintop">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textSize">@dimen/text_size_20</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="logincontentedit">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textCursorDrawable">@drawable/my_cursor</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">@dimen/text_size_16</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@null</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="logincommtextview">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/text_size_16</item>
        <item name="android:textColor">@color/gray_list_head_text</item>
    </style>

    <style name="logincommviewline">
        <item name="android:background">@color/view_line_color</item>
    </style>
</resources>