<?xml version="1.0" encoding="utf-8"?><!--<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"--><!--xmlns:card_view="http://schemas.android.com/apk/res-auto"--><!--android:id="@+id/card_view"--><!--android:layout_width="100mm"--><!--android:layout_height="120mm"--><!--android:layout_gravity="center"--><!--android:layout_marginLeft="5dp"--><!--android:layout_marginRight="5dp"--><!--android:layout_marginBottom="-20mm"--><!--android:clipChildren="true"--><!--android:foreground="?android:attr/selectableItemBackground"--><!--card_view:cardBackgroundColor="@color/item_bg"--><!--card_view:cardCornerRadius="8dp"--><!--card_view:cardElevation="6dp"--><!--card_view:contentPadding="0mm"--><!--card_view:cardPreventCornerOverlap="false"--><!--card_view:cardUseCompatPadding="true">-->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="120mm"
    android:layout_height="120mm"
    android:layout_gravity="center"
    android:layout_marginLeft="0mm"
    android:layout_marginTop="20mm"
    android:layout_marginRight="0mm"
    android:layout_marginBottom="-22mm"
    android:clipChildren="true">
    <!--android:background="@drawable/slb_books_yy1"-->
    <FrameLayout
        android:layout_width="114mm"
        android:layout_height="120mm"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="2mm">

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:cardBackgroundColor="#1D272727"
            app:cardCornerRadius="8mm"
            app:cardElevation="2mm"
            app:cardPreventCornerOverlap="false"
            app:cardUseCompatPadding="true" />
        <!--#1D272727-->
    </FrameLayout>

    <com.geek.libglide47.base.GlideImageView
        android:id="@+id/brademo1_img"
        android:layout_width="110mm"
        android:layout_height="110mm"
        android:layout_centerInParent="true"
        android:scaleType="centerCrop"
        app:siv_border_color="@color/white"
        app:siv_border_width="0mm"
        app:siv_pressed_alpha="0.5"
        app:siv_pressed_color="@color/white_alpha30"
        app:siv_radius="8mm"
        app:siv_shape_type="rectangle" />

    <!--<com.geek.libglide47.base.GlideImageView-->
    <!--android:id="@+id/brademo1_img"-->
    <!--android:layout_width="96mm"-->
    <!--android:layout_height="match_parent"-->
    <!--android:layout_alignParentStart="true"-->
    <!--android:layout_alignParentLeft="true"-->
    <!--android:layout_alignParentTop="true"-->
    <!--android:scaleType="centerCrop"-->
    <!--android:src="@drawable/animation_img1" />-->

    <RelativeLayout
        android:layout_width="110mm"
        android:layout_height="110mm"
        android:layout_centerInParent="true">

        <com.geek.libglide47.base.GlideImageView
            android:id="@+id/iv_bktt1"
            android:layout_width="40mm"
            android:layout_height="40mm"
            android:layout_marginLeft="0mm"
            android:layout_marginTop="0mm" />
        <!--android:src="@drawable/slb_bktt_icon"-->


        <com.geek.libglide47.base.GlideImageView
            android:id="@+id/iv_bktt2"
            android:layout_width="30mm"
            android:layout_height="30mm"
            android:layout_alignParentRight="true"
            android:layout_marginTop="0mm"
            android:layout_marginRight="0mm" />
        <!--android:src="@drawable/slb_xm_icon"-->

        <com.geek.libglide47.base.GlideImageView
            android:id="@+id/iv_bktt3"
            android:layout_width="40mm"
            android:layout_height="40mm"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="0mm"
            android:layout_marginBottom="0mm" />

        <com.geek.libglide47.base.GlideImageView
            android:id="@+id/iv_bktt4"
            android:layout_width="30mm"
            android:layout_height="30mm"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginRight="0mm"
            android:layout_marginBottom="0mm" />
        <!--android:src="@drawable/slb_vip_icon22"-->

    </RelativeLayout>

    <TextView
        android:id="@+id/tv_tips123"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_gravity="right"
        android:layout_marginTop="2mm"
        android:layout_marginRight="10mm"
        android:background="@drawable/slb_my_btn_comm_you"
        android:paddingLeft="6mm"
        android:paddingRight="6mm"
        android:text="推荐"
        android:textColor="@color/white"
        android:textSize="10mm"
        android:visibility="gone"
        tools:visibility="gone" />

    <ImageView
        android:id="@+id/iv_del"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_gravity="right"
        android:layout_marginRight="2mm"
        android:src="@drawable/children_lock_close"
        android:visibility="gone"
        tools:visibility="gone" />
</RelativeLayout>

    <!--</android.support.v7.widget.CardView>-->
