<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="10mm">

    <LinearLayout
        android:id="@+id/rl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="horizontal"
        android:padding="10mm"
        android:visibility="gone">

        <LinearLayout
            android:layout_width="0mm"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv11"
                android:layout_width="60mm"
                android:layout_height="60mm"
                android:background="@drawable/btn_common_blue"
                android:gravity="center"
                android:text="1"
                android:textColor="@color/white"
                android:textSize="16mm" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="6mm"
                android:text="精品绘本"
                android:textSize="12mm" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0mm"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv22"
                android:layout_width="60mm"
                android:layout_height="60mm"
                android:background="@drawable/btn_common_blue"
                android:gravity="center"
                android:text="1"
                android:textColor="@color/white"
                android:textSize="16mm" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="6mm"
                android:text="普通绘本"
                android:textSize="12mm" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0mm"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv33"
                android:layout_width="60mm"
                android:layout_height="60mm"
                android:background="@drawable/btn_common_blue"
                android:gravity="center"
                android:text="1"
                android:textColor="@color/white"
                android:textSize="16mm" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="6mm"
                android:text="下载绘本"
                android:textSize="12mm" />
        </LinearLayout>
    </LinearLayout>


    <RelativeLayout
        android:id="@+id/rl0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10mm"
        android:background="@drawable/slb_my_btn_comm"
        android:padding="10mm"
        android:visibility="gone">

        <TextView
            style="@style/logincommtextview"
            android:layout_centerVertical="true"
            android:drawableLeft="@drawable/slb_fankui"
            android:drawablePadding="10mm"
            android:gravity="center_vertical"
            android:text="我的订单"
            android:textColor="@color/black_000"
            android:textSize="16mm" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/slb_back" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:visibility="gone">

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5mm"
            android:layout_marginLeft="16mm"
            android:layout_marginRight="16mm"
            android:background="@color/view_line_colorEBEBEB"
            android:visibility="visible" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rlvip1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/slb_my_btn_comm_shang"
        android:padding="12mm">

        <TextView
            style="@style/logincommtextview"
            android:layout_centerVertical="true"
            android:drawableLeft="@drawable/slb_me_vip2"
            android:drawablePadding="10mm"
            android:gravity="center_vertical"
            android:text="合象会员"
            android:textColor="@color/black_000"
            android:textSize="16mm" />

        <TextView
            android:layout_width="20mm"
            android:layout_height="20mm"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/slb_back" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5mm"
            android:layout_marginLeft="12mm"
            android:layout_marginRight="12mm"
            android:background="@color/view_line_colorEBEBEB"
            android:visibility="visible" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rldd2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/slb_my_btn_comm"
        android:padding="12mm">

        <TextView
            style="@style/logincommtextview"
            android:layout_centerVertical="true"
            android:drawableLeft="@drawable/slb_me_dingdan2"
            android:drawablePadding="10mm"
            android:gravity="center_vertical"
            android:text="我的订单"
            android:textColor="@color/black_000"
            android:textSize="16mm" />

        <TextView
            android:layout_width="20mm"
            android:layout_height="20mm"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/slb_back" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5mm"
            android:layout_marginLeft="12mm"
            android:layout_marginRight="12mm"
            android:background="@color/view_line_colorEBEBEB"
            android:visibility="visible" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/slb_my_btn_comm"
        android:padding="12mm">

        <TextView
            style="@style/logincommtextview"
            android:layout_centerVertical="true"
            android:drawableLeft="@drawable/me_about2"
            android:drawablePadding="10mm"
            android:gravity="center_vertical"
            android:text="关于我们"
            android:textColor="@color/black_000"
            android:textSize="16mm" />

        <TextView
            android:layout_width="20mm"
            android:layout_height="20mm"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/slb_back" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5mm"
            android:layout_marginLeft="12mm"
            android:layout_marginRight="12mm"
            android:background="@color/view_line_colorEBEBEB"
            android:visibility="visible" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/slb_my_btn_comm"
        android:padding="12mm">

        <TextView
            style="@style/logincommtextview"
            android:layout_centerVertical="true"
            android:drawableLeft="@drawable/me_kefu2"
            android:drawablePadding="10mm"
            android:gravity="center_vertical"
            android:text="反馈中心"
            android:textColor="@color/black_000"
            android:textSize="16mm" />

        <TextView
            android:layout_width="20mm"
            android:layout_height="20mm"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/slb_back" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5mm"
            android:layout_marginLeft="12mm"
            android:layout_marginRight="12mm"
            android:background="@color/view_line_colorEBEBEB"
            android:visibility="visible" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/slb_my_btn_comm_xia"
        android:padding="12mm">

        <TextView
            style="@style/logincommtextview"
            android:layout_centerVertical="true"
            android:drawableLeft="@drawable/me_set2"
            android:drawablePadding="10mm"
            android:gravity="center_vertical"
            android:text="设置"
            android:textColor="@color/black_000"
            android:textSize="16mm" />

        <TextView
            android:layout_width="20mm"
            android:layout_height="20mm"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/slb_back" />
    </RelativeLayout>

</LinearLayout>